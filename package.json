{
  "name": "app-builder",
  "version": "0.0.1",
  "description": "Lucrare Licenta CTI UGAL 2019 Florin Munteanu",
  "main": "App.js",
  "scripts": {
    "start": "electron dist/App.js",
    "cs": "npm run compile && npm start",
    "compile": "grunt copy && grunt ts",
    "compile-electron": "electron-packager ./dist/ app-builder --platform linux --arch x64 --out ./out --overwrite",
    "create-deb": "electron-installer-debian --src ./out/app-builder-linux-x64 --dest ./out/installers/ --arch amd64",
    "package": "npm run compile && npm run compile-electron && npm run create-deb",
    "watch": "grunt watch"
  },
  "keywords": [
    "licenta",
    "ugal",
    "cti"
  ],
  "author": "Florin Munteanu",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^16.8.22",
    "@types/underscore": "^1.9.2",
    "electron": "^4.0.5",
    "electron-installer-debian": "^2.0.0",
    "electron-installer-windows": "^2.0.0",
    "electron-packager": "^13.1.0",
    "grunt": "^1.0.3",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-shell": "^3.0.1",
    "grunt-ts": "^6.0.0-beta.22",
    "lodash": "^4.17.19",
    "npx": "^10.2.0",
    "typescript": "3.5.2"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.21",
    "@fortawesome/free-solid-svg-icons": "^5.10.1",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "androidjs": "^2.0.1",
    "androidjs-builder": "^2.0.2",
    "brace": "^0.11.1",
    "jquery": "^3.3.1",
    "nano-markdown": "^1.2.0",
    "ncp": "^2.0.0",
    "react": "^16.8.6",
    "react-ace": "^7.0.2",
    "react-block-ui": "^1.3.1",
    "react-contextmenu": "^2.11.0",
    "react-device-simulator": "^1.0.1",
    "react-dom": "^16.8.6",
    "react-frame-component": "^4.1.1",
    "react-simple-checkbox": "^1.3.0",
    "react-sortable-tree": "^2.6.2",
    "react-sortable-tree-theme-full-node-drag": "^1.1.0",
    "styled-components": "^4.3.2",
    "underscore": "^1.9.1"
  }
}
